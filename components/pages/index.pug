extends ../base/_skeleton

include ../rd-navbar/_rd-navbar
include ../quote/_quote
include ../post/_post
include ../card/_card

block variables
	- pageName = 'Home';

block page
	+rd-navbar
	
	section.section.section-custom.bg-dark.context-dark.bg-image.parallax-js.parallax-js-box-1(style='background-image: url("images/bg-image-01.png"); background-position: center bottom;')

		.parallax-js-element-1: .layer( data-depth='.1' )
			img(src="images/pjs-element-1.svg", alt="")
		.parallax-js-element-2: .layer( data-depth='-.1' )
			img(src="images/pjs-element-2.svg", alt="")
		.parallax-js-element-3: .layer( data-depth='.1' )
			img(src="images/pjs-element-3.svg", alt="")
		.parallax-js-element-4: .layer( data-depth='-.1' )
			img(src="images/pjs-element-4.svg", alt="")
		.parallax-js-element-5: .layer( data-depth='.1' )
			img(src="images/pjs-element-arrow.svg", alt="")
		.container
			.row.row-40.align-items-md-end
				.col-md-6.col-lg-6.offset-md-90
					h1 Transform your business.  Take back your life.
					p Take a chance and explore the world of public speaking! Join us right now!
					a.btn.btn-primary(href='#') Get Started
				.col-md-5.col-lg-5.pl-xl-0
					+quote-minimal({
						name: 'Adam Green',
						position: 'Business coach'
					},'It’s time to make your business successful.')

	section.section.section-special-1
		.container
			.row.row-40
				.col-md-6.col-xl-5
					h2.section-special-1-title Proven systems. A personal mentor.
					p Becoming the best is a learning process. Find out how you can make your business thrive with these simple yet powerful tips.

				.col-md-6.col-xl-7
					.owl-style-1-container
