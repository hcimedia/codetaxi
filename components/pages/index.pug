						.owl-carousel.owl-style-1( data-owl= { dots: true, stagePadding: 0, margin: 0, dotsEach: 1  }, data-items='1', data-xl-items='2', data-loop='true' ) 
							+post-minimal({
								img: 'post-minimal-1-400x297',
								category: 'Teamwork',
								title: 'What Does A Business Coach Do?'
							})
							+post-minimal({
								img: 'post-minimal-2-400x297',
								category: 'Tips',
								title: 'Build the right team working the right way'
							})
							+post-minimal({
								img: 'post-minimal-3-400x297',
								category: 'Startup',
								title: 'What Can A Business Coach Help With?'
							})
							+post-minimal({
								img: 'post-minimal-4-400x297',
								category: 'Coaching',
								title: 'Is A Small Business Coach Different?'
							})

