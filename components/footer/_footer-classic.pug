include ../post/_post
include ../rights/_rights

// footer classic
footer.footer.section.section-xl.bg-white.footer-classic
	.container
		.row.row-50.justify-content-between
			.col-lg-5.col-xl-4
				+post-video({
					videoLink: 'https://www.youtube.com/embed/OjNS4YjWILo',
					img: 'post-video-1-400x295',
					category: 'Client Stories',
					title: 'Achieving Success as a Small Business'
				})
			.col-lg-7.col-xl-6
				.row.row-30
					.col-6.col-md-4
						h6 About Us
						ul.list
							li: a( href='#' ) Who We Are
							li: a( href='#' ) Our Approach
							li: a( href='solutions.html' ) Solutions
							li: a( href='client-stories.html' ) Client Stories
							li: a( href='#' ) Brand Identity
							li: a( href='#' ) Become a Coach
							li: a( href='#' ) Request a Speaker
					.col-6.col-md-4
						h6 Coaching
						ul.list
							li: a( href='#' ) How It Works
							li: a( href='#' ) Program Roadmap
							li: a( href='#' ) Our Coaches
							li: a( href='#' ) Start Now
					.col-6.col-md-4
						h6 Help
						ul.list
							li: a( href='#' ) Help Center
							li: a( href='#' ) FAQs
							li: a( href='#' ) Terms
							li: a( href='contacts.html' ) Contact Us
				.footer-classic-form
					h6 Sign up for our newsletter
					form.rd-mailform.form-inline-custom( data-form-output='form-output-footer-store', data-form-type='subscribe', method='post', action='components/rd-mailform/rd-mailform.php' )
						.form-group.novi-disabled
							input.form-control( type='email', name='email', placeholder='Enter your email', data-constraints='@Email @Required' )
							button.btn.btn-arrow.btn-arrow-primary( type='submit' ) Submit
					#form-output-footer-store.form-output.snackbar.snackbar-primary

	.footer-classic-bottom
		.container
			.row.row-30.align-items-center
				.col-lg-6
					+rights
				.col-lg-6
					ul.list.social-icon-list.list-inline.list-inline-lg
						li.list-inline-item: a.fa-rss( href='#' )
						li.list-inline-item: a.fa-twitter( href='#' )
						li.list-inline-item: a.fa-facebook-f( href='#' )
						li.list-inline-item: a.fa-linkedin-square( href='#' )
